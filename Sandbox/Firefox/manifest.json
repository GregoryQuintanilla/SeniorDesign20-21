{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Building an Extenstion!",
    "permissions": [ 
        "activeTab",
        "storage",
        "https://www.google.com/",
        "https://*/",
        "https://localhost:5000/toServer" 
        ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "Home.html",
        "default_icon": {
            "48": "./Images/report.png"
        }
    },
    "icons": {
        "48": "./Images/report.png"
    },

    "content_scripts":[
        {
            "matches":["*://*/*"],
            "js":["background.js"]
        }
    ],

    "manifest_version":2
}