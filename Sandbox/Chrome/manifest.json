{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Building an Extenstion!",
    "permissions": [ 
        "activeTab",
        "declarativeContent", 
        "storage",
        "https://www.google.com/",
        "https://*/",
        "https://localhost:5000/toServer" 
        ],
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imgs/Logo.png",
            "32": "imgs/Logo.png",
            "48": "imgs/Logo.png",
            "128": "imgs/Logo.png"
        }
    },
    "icons": {
        "16": "imgs/Logo.png",
        "32": "imgs/Logo.png",
        "48": "imgs/Logo.png",
        "128": "imgs/Logo.png"
    },
    "manifest_version":2
}